{
  "name": "STi",
  "short_name": "STi",
  "author": "ZergRael",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Sooo, let's DL moar ?",
  "icons": { 
    "16": "images/sti.16.png",
    "48": "images/sti.48.png",
    "128": "images/sti.128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.so-torrent.com/*"],
      "js": [
        "lib/jquery-2.1.4.min.js",
        "lib/sti_utils.js",
        "options.js",
        "sti.js",
        "module/global.module.js",
        "module/endless_scrolling.module.js",
        "module/torrent_list.module.js",
        "module/sphinx.module.js",
        "module/snatched.module.js",
        "module/torrent.module.js",
        "module/twits.module.js",
        "module/forums.module.js",
        "module/badges.module.js",
        "module/logs.module.js",
        "module/bookmark.module.js",
        "module/m_account.module.js",
        "loader.js"
      ]
    }
  ],
  "permissions": [ 
    "storage"
  ],
  "web_accessible_resources": [
    "images/loading.gif",
    "images/bookmark.png",
    "images/nfo.png",
    "images/watch.png",
    "images/watch_new.png",
    "images/Classic/to_top_small.png",
    "images/Classic/endless_scrolling_active.png",
    "images/Classic/endless_scrolling_paused.png",
    "images/LordVal/to_top_small.png",
    "images/LordVal/endless_scrolling_active.png",
    "images/LordVal/endless_scrolling_paused.png"
  ]
}